    <script>
        $("form").submit(function (e) {
            e.preventDefault();
        });
        $("#cf-submit").click(function () {
            console.log("clicked")
            $.ajax({
                type: "POST",
                url: "/messagePost",
                data: { email: $("#cf-email").val(), name: $("#cf-name").val(), subject: $("#cf-subject").val(), message: $("#cf-message").val() },
                success: function (data) {
                    window.alert("message sent successfully!!!");
                    $("#cf-email").val("")
                    $("#cf-name").val("")
                    $("#cf-subject").val("")
                    $("#cf-message").val("")
                }
            })
        })
