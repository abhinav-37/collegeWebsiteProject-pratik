                            <% } %>

                            <!-- <div class="tst-author">
                                    <h4>Digital Carlson</h4>
                                    <span>Pharetra quam sit amet</span>
                                </div> -->
                        </div>
                        <% } }else if(length<4 && length > 0 ){ %>
                        <% for(var j= 0; j<length; j++){ %>
                        <div class="item notice-show">
                            <p><%= notice[j].notice %></p>
                            <% if(notice[j].drive){ %>
                            <a class="notive-a" target="_blank" href=<%= notice[j].drive %>>Click to open the notice</a>
